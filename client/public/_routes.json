{
  "version": 1,
  "include": [
    "/*"
  ],
  "exclude": [
    "/assets/*",
    "/build/*",
    "/*.ico",
    "/*.png",
    "/*.svg",
    "/*.jpg",
    "/*.jpeg",
    "/*.gif",
    "/*.json",
    "/*.js",
    "/*.css"
  ],
  "routes": [
    {
      "src": "^/auth.*",
      "dest": "/index.html"
    },
    {
      "src": "^/[^.]*$",
      "dest": "/index.html"
    }
  ]
} 